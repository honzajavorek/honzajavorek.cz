    "created_at": "2023-10-04T07:42:15.964000+00:00",
    "in_reply_to_id": 111175595060968806,
    "in_reply_to_account_id": 110921870573231541,
    "sensitive": false,
    "spoiler_text": "",
    "visibility": "public",
    "language": "cs",
    "uri": "https://mastodonczech.cz/users/honzajavorek/statuses/111175620097798055",
    "url": "https://mastodonczech.cz/@honzajavorek/111175620097798055",
    "replies_count": 1,
    "reblogs_count": 0,
